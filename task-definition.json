{
    "containerDefinitions": [ 
       { 
          "essential": true,
          "image": "ghcr.io/daniel55247/try2/imagefstry2:latest",
          "name": "app-container",
          "portMappings": [ 
             { 
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp"
             }
          ]
       }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:ecs:us-east-2:774305593614:cluster/testCluster",
